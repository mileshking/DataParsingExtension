{
  "manifest_version": 2,
  "name": "info parser ext",
  "version": "0.1",
   
 "content_scripts": [{
	"matches": [
		"<all_urls>"
	],
	"js": ["jquery-3.2.1.js"]
 }],

 "browser_action": {
	"default_icon": "icon2.png",
	"default_popup": "parse.html"
 }, 

  "description": "Parse all text from web page w/ formatting.",
  "background": {
	"scripts": ["parse.js"], 
	"persisent": false
	}
  
}
